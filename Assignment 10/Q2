#include <iostream>
#include <unordered_set>
using namespace std;

int main() {
    int arr[] = {0, -1, 2, -3, 1};
    int n = 5, target = -2;

    unordered_set<int> s;

    for(int x : arr) {
        if(s.find(target - x) != s.end()) {
            cout << "true";
            return 0;
        }
        s.insert(x);
    }

    cout << "false";
    return 0;
}
