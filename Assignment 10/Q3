#include <iostream>
#include <unordered_map>
using namespace std;

int main() {
    int a[] = {1, 7, 1};
    int b[] = {7, 7, 1};
    int n = 3;

    unordered_map<int, int> freq;

    for(int x : a) freq[x]++;
    for(int x : b) freq[x]--;

    for(auto p : freq) {
        if(p.second != 0) {
            cout << "false";
            return 0;
        }
    }

    cout << "true";
    return 0;
}
